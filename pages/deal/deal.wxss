.buttons {
  display: flex;
  flex-direction: row;
  align-items: center;
  width: 100%;
  margin-bottom: 20rpx;
}

.button {
  background-color: #f9343e;
  color: white;
  border-radius: 0;
  width: 50%;
  font-size: 14px;
}

.button::after {
  border-radius: 0;
}

.container {
  padding-top: 0rpx;
  background-color: #f5f5f5;
}

.centercolumn {
  display: flex;
  flex-direction: column;
  align-items: center;
}

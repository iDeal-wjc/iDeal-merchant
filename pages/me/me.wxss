.header {
  display: flex;
  flex-direction: row;
  width: 100%;
  height: 164rpx;
  background-color: #fa4f54;
}

.white-circle {
  width: 100rpx;
  height: 100rpx;
  border-radius: 50rpx;
  background-color: white;
  margin-top: 22rpx;
  margin-bottom: 42rpx;
  margin-left: 30rpx;
}

.header-text {
  color: white;
  font-size: 36rpx;
  margin-top: 48rpx;
  margin-bottom: 66rpx;
  margin-left: 20rpx;
}

.row {
  display: flex;
  flex-direction: row;
  align-items: center;
  width: 100%;
  height: 118rpx;
  margin-top: 40rpx;
  background-color: white;
}

.row-left-image {
  width: 48rpx;
  height: 37.6rpx;
  margin-left: 30rpx;
}

.row-text {
  color: #323232;
  font-size: 32rpx;
  margin-left: 16rpx;
}

.row-right-image {
  width: 25rpx;
  height: 42rpx;
  margin-left: 474rpx;
}

page {
  display: block;
  min-height: 100%;
  background-color: #f2f2f2;
}